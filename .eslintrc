{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "arrow-body-style": ["error", "always"],
    "no-restricted-syntax": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-control-regex": 0,
    "consistent-return": 0,
    "newline-per-chained-call": 0,
    "guard-for-in": 0,
    "prefer-const": 0,
    "no-underscore-dangle": 0
  }
}
